
help:
	@echo "Options: clean help install setupdb"

install:
	@echo "Installing... (sorry, not available in prealpha)"

clean:
	@echo "Removing database..."
	@rm -f ./ln.db
	@rm -f /home/*/.lightnews.db
	@echo "Removing *.pyc files..."
	@rm -f ./*.pyc
	@echo "Removing Makefile..."
	@rm -f ./Makefile
	@echo "Done"

